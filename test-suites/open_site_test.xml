<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="open_site_suite">
    <test name="search_in_google_test">
        <groups>
            <run>
                <include name="searchInGoogleTest"/>
            </run>
        </groups>
        <classes>
            <class name="com.tnt.testing.selenide.TNTechOpenSiteTest"/>
        </classes>
    </test>

    <test name="open_contacts_test">
        <groups>
            <run>
                <include name="openContactsTest"/>
            </run>
        </groups>
        <classes>
            <class name="com.tnt.testing.selenide.TNTechOpenSiteTest"/>
        </classes>
    </test>
</suite>